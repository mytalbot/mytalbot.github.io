@article{RefWorks:RefID:47-van2021measuring,
	author={Stevie Van der Mierden and Cathalijn H. C. Leenaars and Erin C. Boyle and Florenza L. Ripoli and Peter Gass and Mattea Durst and Vivian C. Goerlich-Jansson and Paulin Jirkof and Lydia M. Keubler and Steven R. Talbot and Anne Habedank and Lars Lewejohann and Rene H. Tolba and Ané Bleich and dASS BW-2 and AISS Animal Behaviour},
	year={2021},
	month={Jan 01},
	title={Measuring endogenous corticosterone in laboratory mice - a mapping review, meta-analysis, and open source database},
	journal={ALTEX, alternatives to animal experimentation},
	volume={38},
	number={1},
	pages={111-122},
	abstract={Evaluating stress in laboratory animals is a key principle in animal welfare. Measuring corticosterone is a common method to assess stress in laboratory mice. There are, however, numerous methods to measure glucocorticoids with differences in sample matrix (e.g., plasma, urine) and quantification techniques (e.g., enzyme immunoassay or radioimmunoassay). Here, the authors present a mapping review and a searchable database, giving a complete overview of all studies mea­suring endogenous corticosterone in mice up to February 2018. For each study, information was recorded regarding mouse strain and sex; corticosterone sample matrix and quantification technique; and whether the study covered the research theme animal welfare, neuroscience, stress, inflammation, or pain (the themes of specific interest in our con­sortium). Using all database entries for the year 2012, an exploratory meta-regression was performed to determine the effect of predictors on basal corticosterone concentrations. Seventy-five studies were included using the predictors sex, time-since-lights-on, sample matrix, quantification technique, age of the mice, and type of control. Sex, time-since-lights-on, and type of control significantly affected basal corticosterone concentrations. The resulting database can be used, inter alia, for preventing unnecessary duplication of experiments, identifying knowledge gaps, and standardizing or heterogenizing methodologies. These results will help plan more efficient and valid experiments in the future and can answer new questions in silico using meta-analyses.},
	keywords={my_paper},
	isbn={1868-596X},
	url={https://www.narcis.nl/publication/RecordID/oai:dspace.library.uu.nl:1874\%2F409837},
	doi={10.14573/altex.2004221},
	pmid={33086382}
}
